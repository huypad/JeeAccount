{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "KafkaConfig": {
    "ProjectName": "jee-account",
    "TopicProduce": {
      "JeeplatformInitialization": "jeeplatform.initialization",
      "JeeplatformPostcomment": "jeeplatform.postcomment"
    },
    "TopicConsume": {
      "JeeplatformInitializationAppupdate": "jeeplatform.initialization.appupdate"
    },
    "Consumer": {
      "JeeAccountGroup": "jeeaccount-consumer-group"
    }
  },
  "MinioConfig": {
    "MinioSSL": "true",
    "MinioServer": "cdn.jee.vn"
  },
  "Host": {
    "JeeComment_API": "https://jeecomment-api.jee.vn",
    "JeeHR_API": "https://jeehr-api.jee.vn"
  }
}