{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "KafkaConfig": {
    "ProjectName": "jee-account",
    "TopicProduce": {
      "JeeplatformInitialization": "jeeplatform.initialization",
      "JeeplatformPostcomment": "jeeplatform.postcomment",
      "JeeplatformUpdateAdmin": "jeeplatform.updateadmin"
    },
    "TopicConsume": {
      "JeeplatformInitializationAppupdate": "jeeplatform.initialization.appupdate",
      "JeeplatformUpdateadminAppupdate": "jeeplatform.updateadmin.appupdate"
    },
    "Consumer": {
      "JeeAccountGroup": "jeeaccount-consumer-group",
      "JeePermissionGroup": "permission-consumer-group"
    }
  },
  "MinioConfig": {
    "MinioSSL": "true"
  }
}